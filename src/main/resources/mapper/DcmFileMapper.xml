<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dcmmanagesystem.dao.DcmFileMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.dcmmanagesystem.model.DcmFile">
        <id column="file_id" property="fileId" />
        <result column="project_id" property="projectId" />
        <result column="file_path" property="filePath" />
        <result column="upload_date" property="uploadDate" />
        <result column="upload_user_id" property="uploadUserId" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        file_id, project_id, file_path, upload_date, upload_user_id
    </sql>

</mapper>

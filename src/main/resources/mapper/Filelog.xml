<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dcmmanagesystem.dao.FilelogMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.dcmmanagesystem.model.Filelog">
        <id column="id" property="id" />
        <result column="file_name" property="fileName" />
        <result column="upload_user_id" property="uploadUserId" />
        <result column="file_path" property="filePath" />
        <result column="upload_date" property="uploadDate" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, file_name, upload_user_id, file_path, upload_date
    </sql>

</mapper>
